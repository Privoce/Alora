{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "description": "__MSG_extDescription__",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "cookies",
    "storage",
    "tabs",
    "history",
    "webRequest",
    "webRequestBlocking",
    "chrome://favicon/",
    "*://*/*"
  ],
  "content_security_policy": "script-src 'self';object-src 'self';img-src chrome://favicon;",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "circle-default.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "default_locale": "en"
}
